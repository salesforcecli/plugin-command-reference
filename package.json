{
  "name": "@salesforce/plugin-command-reference",
  "description": "Generate the Salesforce CLI command reference guide",
  "version": "2.0.0",
  "author": "Thomas @amphro",
  "bugs": "https://github.com/salesforcecli/plugin-command-reference/issues",
  "dependencies": {
    "@oclif/core": "^0.5.34",
    "@salesforce/command": "^4.0.5",
    "@salesforce/core": "^2.2.0",
    "@salesforce/kit": "^1.5.17",
    "@salesforce/ts-types": "^1.5.20",
    "chalk": "^3.0.0",
    "handlebars": "^4.7.7",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-help": "^2.2.3",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "^0.9.18",
    "@salesforce/plugin-login": "^0.0.18",
    "@salesforce/prettier-config": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.3.8",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "oclif": "^1.18.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "shx": "^0.3.3",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/templates"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-command-reference",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "topicSeparator": " ",
    "topics": {
      "commandreference": {
        "description": "generate the Salesforce CLI command reference guide.",
        "longDescription": "xx"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@salesforce/plugin-login"
    ]
  },
  "repository": "salesforcecli/plugin-command-reference",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "command-reference": "./bin/dev commandreference:generate --plugins login --outputdir test/tmp",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test && yarn command-reference",
    "test": "sf-test",
    "version": "oclif readme"
  },
  "husky": {
    "hooks": {
      "commit-msg": "sf-husky-commit-msg",
      "pre-commit": "sf-husky-pre-commit",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
